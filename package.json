{
	"private": true,
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"build": "dotenv -- turbo run build",
		"deploy": "dotenv -- bash -c 'cd ./scripts/deploy/ansible/ && ./deploy.sh'",
		"supa_stop": "dotenv -- turbo run supa_stop",
		"dev": "dotenv -- turbo run supa_start && turbo run dev",
		"convert_to_yarn": "echo 'CMD: npx @turbo/workspaces convert https://twitter.com/anthonysheww/status/1736045947111817631'"
	},
	"devDependencies": {
		"turbo": "latest",
		"dotenv-cli": "latest"
	},
	"packageManager": "pnpm@8.9.0",
	"engines": {
		"node": ">=18"
	}
}
