{
	"globalDotEnv": [".env", ".env.development", ".env.production"],
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".svelte-kit/**", "dist/**"]
		},
		"lint": {},
		"supa_start": {
			"cache": false
		},
		"supa_stop": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
